<script>
// JavaScript function to open the link in a new tab
function redirectToLink(url) {
window.open(url, "_blank"); // Open the specified URL in a new tab
}
</script>

<script>
/* ------------- TOGGLE SERVICE PRICING ------------- */
const spTab = document.querySelectorAll(".spt");
const spBox = document.querySelectorAll(".sp-cards");

// Add a click event listener to each sidebar element
spTab.forEach((element, index) => {
element.addEventListener("click", () => {
// Remove active class from all sidebar elements
spTab.forEach((sptt) => {
sptt.classList.remove("active");
});
// Add active class to the clicked sidebar element
element.classList.add("active");

// Remove active class from all containers
spBox.forEach((spbb) => {
spbb.classList.remove("active");
});
// Add active class to the container with the same index as the clicked sidebar element
spBox[index].classList.add("active");
});
});
</script>


