<script>
const prTab = document.querySelectorAll(".ppt");
const prBox = document.querySelectorAll(".ppw");

// Add a click event listener to each sidebar element
prTab.forEach((element, index) => {
element.addEventListener("click", () => {
// Remove active class from all sidebar elements
prTab.forEach((prt) => {
prt.classList.remove("active");
});
// Add active class to the clicked sidebar element
element.classList.add("active");

// Remove active class from all containers
prBox.forEach((prb) => {
prb.classList.remove("active");
});
// Add active class to the container with the same index as the clicked sidebar element
prBox[index].classList.add("active");
});
});
</script>

